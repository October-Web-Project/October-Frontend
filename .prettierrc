{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "importOrder": [
    "^react",
    "^next",
    "^(react|lodash|dayjs|axios).*",
    "^@constants/.*$",
    "^@store/.*$",
    "^@apis/.*$",
    "^@utils/.*$",
    "^@hooks/.*$",
    "^@components/.*$",
    "^@/.*",
    "^[.].*/.*",
    ".*"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
